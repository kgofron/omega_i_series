record(ai, "$(Sys)$(Dev)T$(N_T)-Raw:I")
{
   field(DTYP, "stream")
   field(DESC, "Process Temperature")
   field(INP, "@omega-CNi32.proto ReadX($(Chan),01) $(PORT)")
   field(EGU,  "Celsius")
   field(SCAN, "$(PERIOD=10) second")
   field(HIHI, "35")
   field(HIGH, "30")
   field(LOLO, "10")
   field(LOW,  "15")
   field(HOPR, "35")
   field(LOPR, "15")
   field(PREC, "1")
   field(HHSV, "MAJOR")
   field(HSV,  "MINOR")
   field(LLSV, "MINOR")
   field(LSV,  "MINOR")
   field(FLNK, "$(Sys)$(Dev)T$(N_T)-Process")

   info(autosaveFields, "LOLO LOW HIGH HIHI LLSV LSV HSV HHSV")
}


record(aSub, "$(Sys)$(Dev)T$(N_T)-Process"){
	field(SNAM, "filter")

	field(INPA, "$(Sys)$(Dev)T$(N_T)-Raw:I")
	field(FTA, "DOUBLE")

	field(OUTA, "$(Sys)$(Dev)T$(N_T)-I PP")
	field(FTVA, "DOUBLE")

}

record(aSub, "$(Sys)$(Dev)T$(N_T)-RollCall"){
	field(SNAM, "rollCall")
	field(INPA, "$(Sys)$(Dev)T$(N_T)-I.STAT")
	field(INPB, "$(Sys)$(Dev)T$(N_T)-I.NAME")
	field(FTA, "STRING")
	field(FTB, "STRING")
}



